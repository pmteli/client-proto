<script type="text/javascript">
    $( document ).ready(function() {
        $("#energy_care").ionRangeSlider({
            values: [
                "Not Much", "Somewhat",
                "A Lot"
            ],
            type: 'double',
            from: 'Somewhat',
            to: 'Somewhat',
            //hasGrid: true
        });
        $("#no_employees").ionRangeSlider({
            min: 1,
            max: 1000,
            from: 1,
            type: 'single',
            step: 5,
            postfix: "",
            prettify: true,
            hasGrid: true
        });
    });
</script>


